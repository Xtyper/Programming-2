#include <stdio.h>
int main()
{
   int first, second, add, subtract, multiply;
   float divide;
 
   printf("Input any 2 integers ");
   scanf("%d%d", &first, &second);
 
   add = first + second;
   multiply = first * second;
   divide = first / second; 
   subtract = first - second;
   

   printf("ADD = %d\n", add);
   printf("MUL = %d\n", multiply);
   printf("DIV = %.1f\n", divide); 
   printf("SUB = %d\n", subtract);
   
 
   return 0;
}
